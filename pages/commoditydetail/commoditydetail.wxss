.quick-nav {
  display: block;
  position: fixed;
  bottom: 140rpx;
  right: 30rpx;
  z-index: 99;
}

.quick-nav .nav {
  width: 94rpx;
  height: 94rpx;
  margin: 15rpx 0;
  background: #f96b81;
  opacity: 0.9;
  border: 0;
  border-radius: 50%;
  text-align: center;
  position: absolute;
  bottom: 80rpx;
  right: 0;
  z-index: -1;
}

.quick-nav .nav._main {
  display: block;
  z-index: 1;
  font-size: 20rpx;
  background: #000;
  opacity: 0.5;
  color: #fff;
}

.quick-nav .nav._close {
  background: #fff;
  box-shadow: 0 0 21rpx #d2d2d2;
  /* position: relative; */
}

.nav._main view {
  width: 50rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.nav._close image {
  width: 39rpx;
  height: 39rpx;
  margin-top: 27.5rpx;
}

.nav._home image {
  width: 38rpx;
  height: 40rpx;
  margin-top: 28rpx;
}

.nav._shop image {
  width: 38rpx;
  height: 40rpx;
  margin-top: 28rpx;
}

.nav._user image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 27rpx;
}

.nav._main._hide {
  visibility: hidden;
  display: none;
}

.nav._close._hide {
  visibility: hidden;
  transform: rotate(180deg);
  transition: all 0.2s ease-in-out;
}

.nav._close._show {
  transform: rotate(0deg);
  transition: all 0.2s ease-in-out 0.2s;
}

.nav._home._show {
  z-index: 3;
  transform: translateY(-104rpx);
  transition: all 0.2s ease-in-out 0s;
}

.nav._shop._show {
  z-index: 2;
  transform: translateY(-206rpx);
  transition: all 0.2s ease-in-out 0s;
}

.nav._user._show {
  z-index: 1;
  transform: translateY(-307rpx);
  transition: all 0.3s ease-in-out 0s;
}

.nav._home._hide {
  z-index: -1;
  transform: translateY(0);
  transition: all 0.2s ease-in-out 0s;
  visibility: hidden;
}

.nav._shop._hide {
  z-index: -1;
  transform: translateY(0);
  transition: all 0.2s ease-in-out 0.1s;
  visibility: hidden;
}

.nav._user._hide {
  z-index: -1;
  transform: translateY(0);
  transition: all 0.2s ease-in-out 0.2s;
  visibility: hidden;
}

