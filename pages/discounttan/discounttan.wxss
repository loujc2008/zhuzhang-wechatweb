page {
  height: 100%;
}

.t-tanmask {
  z-index: 99;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.8;
}

.t-tanall {
  z-index: 100;
  box-sizing: border-box;
  padding: 250rpx 108rpx;
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
}

.t-tancontent {
  border-radius: 10rpx;
  box-sizing: border-box;
  width: 536rpx;
  height: 695rpx;
  background-color: #fff;
  padding: 45rpx 30rpx 0 30rpx;
  position:fixed;
}

.t-tanimg1 {
  width: 300rpx;
  height: 105rpx;
  position: absolute;
  right: 225rpx;
  top: 135rpx;
}

.t-tanimg2 {
  width: 71rpx;
  height: 274rpx;
  position: absolute;
  left: 39rpx;
  top: 370rpx;
}

.t-tanimg3 {
  width: 74rpx;
  height: 207rpx;
  position: absolute;
  right: 38rpx;
  top: 615rpx;
}

.t-tanimg4 {
  width: 68rpx;
  height: 118rpx;
  position: absolute;
  right: 345rpx;
  top: 945rpx;
}

.t-unused {
  display: flex;
  /* padding: 30rpx 30rpx 0 30rpx; */
  margin-bottom: 20rpx;
}

/* 空白区域 */

.t-blank {
  position: relative;
}

.t-blankimg {
  width: 474rpx;
  height: 119rpx;
}

.t-blankimg1 {
  width: 340rpx;
  height: 2rpx;
  position: absolute;
  left: 20rpx;
  bottom: 45rpx;
}
.t-bg1 {
  position: absolute;
  height: 857rpx;
  width: 688rpx;
  top: 162rpx;
  left: 14rpx;
}

.t-blanktxt-1-price {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  color: #f36967;
  width: 120rpx;
  text-align: center;
  height: 112rpx;
  line-height: 112rpx;
}

.t-blanktxt-1-price .one {
  font-size: 22rpx;
  font-weight: bold;
}

.t-blanktxt-1-price .two {
  font-size: 51rpx;
  font-weight: bold;
}

.t-blanktxt-2 {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  display: flex;
  flex-direction: column;
  font-size: 16rpx;
  justify-content: space-between;
  box-sizing: border-box;
  width: 190rpx;
  height: 75rpx;
}

.t-blanktxt-2-type {
  color: #f36967;
}

.t-blanktxt-2-count {
  color: #f36967;
}

.t-blanktxt-3 {
  display: flex;
  font-size: 15rpx;
  width: 350rpx;
  justify-content: space-between;
  color: #f36967;
}

.t-blanktxt-3-date {
  width: 186rpx;
  color: #999;
}

.t-clickget {
  width: 404rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius: 50rpx;
  background: #f96269;
  position: fixed;
  bottom: 300rpx;
  left: 170rpx;
  box-shadow: 0rpx 20rpx 20rpx  rgba(249, 98, 105,0.5);
}

.t-tanscroll {
  width: 474rpx;
  height: 70%;
}
